{
  "name": "luasb",
  "version": "0.0.1",
  "description": "LuaSB Storyboard generator",
  "scripts": {
    "lint": "eslint .",
    "dev": "electron app --dev",
    "start": "electron app",
    "test": "mocha tests/test.js",
    "build": "node-sass --output-style compressed --source-map true --output app/ui/css app/ui/scss/main.sass",
    "watch": "node-sass --watch --output-style compressed --source-map true --output app/ui/css app/ui/scss/main.sass"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Kagurame/LuaSB.git"
  },
  "keywords": [
    "lua",
    "sb",
    "storyboard",
    "osu"
  ],
  "author": "Kagu-chan",
  "license": "GPL-3.0",
  "bugs": {
    "url": "http://tracker.kagu-chan.de/projects/luasb/issues"
  },
  "homepage": "http://tracker.kagu-chan.de/projects/luasb",
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "electron": "^1.4.1",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.1",
    "eslint-plugin-react": "^4.2.3",
    "mocha": "^3.2.0",
    "node-sass": "^4.1.1",
    "spectron": "^3.4.1",
    "typings": "^2.0.0"
  },
  "dependencies": {
    "angular": "^1.6.1",
    "angular-animate": "^1.6.1",
    "angular-bootstrap": "^0.12.2",
    "angular-touch": "^1.6.1",
    "angular-ui-bootstrap": "^2.3.1",
    "bluebird": "^3.4.7",
    "bootstrap-sass": "^3.3.7",
    "electron-json-storage": "^2.1.0",
    "fs.promised": "^3.0.0",
    "i18n": "^0.8.3",
    "lodash": "^4.17.2"
  }
}
