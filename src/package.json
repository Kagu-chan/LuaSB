{
  "name": "luasb",
  "version": "0.0.1",
  "description": "LuaSB Storyboard generator",
  "scripts": {
    "lint": "eslint .",
    "test": "electron --debug=5858 app",
    "start": "electron app",
    "build": "node-sass --output-style compressed --source-map true --output app/ui/css app/ui/scss/main.scss",
    "watch": "node-sass --watch --output-style compressed --source-map true --output app/ui/css app/ui/scss/main.scss"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Kagurame/LuaSB.git"
  },
  "keywords": [
    "lua",
    "sb",
    "storyboard",
    "osu"
  ],
  "author": "Kagu-chan",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Kagurame/LuaSB/issues"
  },
  "homepage": "https://github.com/Kagurame/LuaSB#readme",
  "devDependencies": {
    "electron": "^1.4.1",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.1",
    "eslint-plugin-react": "^4.2.3",
    "node-sass": "^4.1.1",
    "nodemon": "^1.11.0",
    "typings": "^2.0.0"
  },
  "dependencies": {
    "angular": "^1.6.0",
    "bluebird": "^3.4.7",
    "fs.promised": "^3.0.0",
    "i18n": "^0.8.3",
    "lodash": "^4.17.2"
  }
}
